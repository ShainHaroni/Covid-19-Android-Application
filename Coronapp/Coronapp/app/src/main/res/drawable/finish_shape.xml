<?xml version="1.0" encoding="utf-8"?>
<shape android:shape="rectangle"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <stroke android:color="@color/finishStroke"
        android:width="3dp"
        android:dashGap="0dp"
        android:dashWidth="15dp"/>
    <gradient
        android:startColor="@color/finishStroke"
        android:endColor="@color/finishSolid"
        android:angle="90"/>
    <corners android:radius="20dp"/>
    <padding android:right="10dp"
        android:left="10dp"
        android:top="10dp"
        android:bottom="10dp"/>
</shape>